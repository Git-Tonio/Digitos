    <script>
        var num1 = prompt("Ingrese el primer numero", "<Escriba aqui>");
        var num2 = prompt("Ingrese el segundo numero", "<Escriba aqui>");

num1.length>num2.length ? alert ("El numero con mas digitos "+num1):"";
num1.length<num2.length ? alert ("El numero con mas digitos "+num2):"";
num1.length==num2.length ? alert ("El numero de digitos es igual "):"";
location.reload();
    </script>
